#!/bin/bash

function swapFiles() {
  echo "### overwrite from $1"
  rm -fr smpl_spec.?
  ln -s $1/smpl_spec.h
  ln -s $1/smpl_spec.c
}

function delFile() {
  if [ -f $1 ]; then
    echo "delete $1"
    rm $1
  fi
}

function linkFile() {
  if [ ! -f $1 ]; then
    echo "link $1"
    ln -s ../toscom/$1
  fi
}

